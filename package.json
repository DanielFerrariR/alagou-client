{
  "name": "alagouClient",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "commit": "git add . && git-cz",
    "check-types": "yarn tsc",
    "prettier": "prettier \"**/*.+(js|jsx|json|ts|tsx|md|mdx|html|eslintrc|prettierrc)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.md,.mdx .",
    "validate": "npm-run-all --parallel check-types check-format lint",
    "start": "react-native start",
    "ios:dev": "react-native run-ios",
    "android:dev": "react-native run-android",
    "android:prod": "react-native run-android --variant=release",
    "android:build:apk": "cd android && ./gradlew assembleRelease",
    "android:build:app": "cd android && ./gradlew bundleRelease",
    "android:key": "keytool -genkeypair -v -keystore my-upload-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.11.0",
